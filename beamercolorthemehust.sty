% color palettes
\setbeamercolor{palette primary}{fg=hustred}
\setbeamercolor{palette secondary}{fg=hustyellow}
% Title page, part, section, subsection
\setbeamercolor{titlelike}{fg=white}
\setbeamercolor{title}{fg=hustred}
\setbeamercolor{partname}{fg=hustred}
%% table of contents
\setbeamercolor{section in toc}{fg=hustred}
\setbeamercolor{subsection in toc}{fg=hustblack}
\setbeamercolor{section number projected}{fg=hustblack}
\setbeamercolor{subsection number projected}{fg=hustblack}
\setbeamercolor{subsubsection number projected}{fg=hustblack}
\setbeamercolor{item}{fg=hustblack,bg=hustyellow}
%% Figures and table
\setbeamercolor{caption}{fg=hustblack}
\setbeamercolor{caption name}{fg=hustred}
%% Elements of a frame
\setbeamercolor{framesubtitle}{bg=hustyellow,fg=white}
\setbeamercolor{frametitle}{bg=hustred,fg=white}
\setbeamercolor{page number in head/foot}{bg=hustred7,fg=white}
%% Blocks
\setbeamercolor{block title}{fg=hustred}

